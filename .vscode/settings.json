{
  "files.associations": {
    "*.css": "tailwindcss",
    ".env*": "dotenv"
  },
  "github.copilot.enable": {
    "*": true,
    "scminput": false,
    ".env*": false,
    "dotenv": false
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ]
}
